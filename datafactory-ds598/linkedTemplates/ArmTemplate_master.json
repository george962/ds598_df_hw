{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datafactory-ds598"
		},
		"GitHub_credential": {
			"type": "secureString",
			"metadata": "Secure string for 'credential' of 'GitHub'"
		},
		"Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Storage'"
		},
		"GitHub_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "george962"
		},
		"HistoricalAirPollution_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://api.openweathermap.org/"
		},
		"HistoricalWeather_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://history.openweathermap.org/"
		},
		"KeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://OpenWeather.vault.azure.net/"
		},
		"Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://ds598storage.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactory-ds598_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"GitHub_credential": {
						"value": "[parameters('GitHub_credential')]"
					},
					"Storage_accountKey": {
						"value": "[parameters('Storage_accountKey')]"
					},
					"GitHub_properties_typeProperties_username": {
						"value": "[parameters('GitHub_properties_typeProperties_username')]"
					},
					"HistoricalAirPollution_properties_typeProperties_url": {
						"value": "[parameters('HistoricalAirPollution_properties_typeProperties_url')]"
					},
					"HistoricalWeather_properties_typeProperties_url": {
						"value": "[parameters('HistoricalWeather_properties_typeProperties_url')]"
					},
					"KeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('KeyVault_properties_typeProperties_baseUrl')]"
					},
					"Storage_properties_typeProperties_url": {
						"value": "[parameters('Storage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}